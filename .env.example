# Vapi Configuration
VAPI_API_KEY=your_vapi_api_key_here

# HubSpot Configuration
HUBSPOT_API_KEY=your_hubspot_private_app_access_token_here

# Server Configuration
PORT=3000
WEBHOOK_BASE_URL=https://your-app.onrender.com

# Optional: Webhook secret for verifying Vapi requests
